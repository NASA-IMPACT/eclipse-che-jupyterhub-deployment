{
  "Comment": "Upsert route 53 record pointing to the VEDA Analytics ELB",
  "Changes": [
    {
      "Action": "${ROUTE53-ACTION}",
      "ResourceRecordSet": {
        "Name": "${QUALIFIER}-analytics.delta-backend.com",
        "Type": "CNAME",
        "TTL": 300,
        "ResourceRecords": [
          {
            "Value": "${ROUTE53-RESOURCE-VALUE}"
          }
        ]
      }
    }
  ]
}